// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Todo {
  id         Int @id @default(autoincrement())
 title       String
 description String
 status      String
 priority    String
 dateTime   DateTime @default(now())
}